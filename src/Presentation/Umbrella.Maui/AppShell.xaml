<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Umbrella.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Umbrella.Maui"
    xmlns:emailDashboard="clr-namespace:Umbrella.Maui.Email.EmailDashboard.Pages"
    
    Shell.FlyoutBehavior="Disabled"
    Title="Umbrella.Maui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate emailDashboard:EmailDashboardPage}"
        Route="MainPage" />

</Shell>
